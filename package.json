{
  "name": "mnemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "normalize-map": "replace \"lib\\\\\\\\\\\\\\\\\" \"lib/\" index.js.map",
    "browserify": "browserify lib/index.js --debug -t [ babelify --presets [ es2015 ] ] | exorcist index.js.map > index.js && npm run normalize-map",
    "pug": "pug index.pug",
    "less": "lessc index.less | postcss --use postcss-import-url --use autoprefixer > index.css",
    "build": "concurrently \"npm run browserify\" \"npm run pug\" \"npm run less\"",
    "watch:js": "watchify lib/index.js --debug --verbose -t [ babelify --presets [ es2015 ] ] -o \"exorcist index.js.map > index.js\"",
    "watch:map": "chokidar \"index.js.map\" -c \"npm run normalize-map\"",
    "watch:pug": "chokidar \"*.pug\" -c \"npm run pug\"",
    "watch:less": "chokidar \"*.less\" -c \"npm run less\"",
    "watch": "concurrently \"npm run watch:js\" \"npm run watch:map\" \"npm run watch:pug\" \"npm run watch:less\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsg-ut/mnemo.git"
  },
  "keywords": [],
  "author": {
    "name": "TSG",
    "email": "info@tsg.ne.jp",
    "url": "http://www.tsg.ne.jp/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsg-ut/mnemo/issues"
  },
  "homepage": "https://github.com/tsg-ut/mnemo#readme",
  "devDependencies": {
    "animejs": "^1.1.0",
    "async": "^2.0.0-rc.6",
    "autoprefixer": "^6.4.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^2.2.0",
    "exorcist": "^0.4.0",
    "gulp": "^3.9.1",
    "jquery": "^3.0.0",
    "less": "^2.7.1",
    "livereload": "^0.4.1",
    "postcss-cli": "^2.5.2",
    "postcss-import-url": "^2.1.0",
    "pug-cli": "^1.0.0-alpha6",
    "replace": "^0.3.0",
    "watchify": "^3.7.0"
  }
}
