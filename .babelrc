{
	env: {
		browser: {
			presets: [
				['env', {
					targets: {
						browsers: [
							'last 2 Chrome versions',
						],
					},
					useBuiltIns: true,
				}],
				'react',
			],
			plugins: [
				'transform-inline-environment-variables',
				'transform-class-properties',
			],
		},
		react: {
			presets: [
				'react',
			],
			plugins: [
				'transform-class-properties',
			],
		},
	},
}
